  "scripts": {
    "test": "yarn run lint && yarn run compile && yarn run jest",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "local": "ts-node-dev --no-notify src/",
    "dev": "yarn run compile && node lib/",
    "build": "export NODE_OPTIONS=--max_old_space_size=4096 && yarn compile",
    "build_local": "SET NODE_OPTIONS=--max_old_space_size=4096&& yarn compile",
    "prod": "NODE_ENV=production node lib/",
    "staging": "NODE_ENV=staging node lib/",
    "staging_local": "SET NODE_ENV=staging&& ts-node-dev --no-notify src/",
    "jest": "jest --forceExit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "jest"
    ],
    "ignore": []
  },
  "types": "lib/",
